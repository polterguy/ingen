/*
 * JumboButton for browsing exhibits.
 */
button
  class:jumbo-button
  innerValue:@"<span>Exhibits</span><span class=""icon-happy""></span>"
  onclick

    /*
     * Dynamically loading exhibits.
     */
    micro.evaluate.file:@INGEN/imp/frontend/exhibits/ui/view-exhibits.hl

    /*
     * Evaluating file responsible for toggling Jumbo Buttons.
     */
    eval-x:x:/+/*
    micro.evaluate.file:@INGEN/imp/common/toggle-jumbo-button.hl
      button:x:/../*/_event?value
